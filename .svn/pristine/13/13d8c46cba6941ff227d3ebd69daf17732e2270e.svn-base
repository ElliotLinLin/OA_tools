html
{
    font-family: sans-serif;

    -webkit-text-size-adjust: 100%;
}

body
{
    margin: 0;
}

/*导航栏*/

.first_Bar{
	position: relative;
	height: 100px;
	background: url(../images/top.png) no-repeat;
	background-size: cover;
	box-shadow: 0px 2px 10px lightgray;
}

.nav_bar2{
	height: 100px;
	background-image: url(../images/top.png);
	box-shadow: 0px 2px 10px lightgray;
}


.nav_bar{
	height: 44px;
	background-color: rgba(0,122,255,0.8);
	box-shadow: 0px 2px 10px lightgray;
}
.nav_bar3{
	height: 44px;
}
.first_Bar .versionChoose{
	position: absolute;
	left:5px;
	bottom: 5px;
	right: 5px;
	width: calc(100% - 10px);
	
	color: white;
	text-align: left;
	padding-left: 55px;
	font-size: medium;
	
	border: none;
	background: url(../images/version.png) no-repeat;
	background-position: 25px center;
	background-size: 20px 20px;
}
.first_Bar .versionChoose1{
	position: absolute;
	left:5px;
	top: 30px;
	right: 5px;
	width: calc(100% - 10px);
	
	color: white;
	text-align: left;
	padding-left: 25px;
	font-size: medium;
	
	border: none;
	background: no-repeat;
	background-position: 25px center;
	background-size: 20px 20px;
}

.first_Bar .versionChooseP{
	position: absolute;
	bottom: 7px;
	right: 30px;
	width: 20px;
	height: 20px;
	
	background: url(../images/version_last.png) no-repeat;
	background-size: contain;
	
}

.first_Bar .leftBack,.nav_bar .leftBack{
	width: 40px;
	height: 44px;
	
	display: inline-block;
}
.leftBack img{
	position: absolute;left: 15px;top: 14px;
	width: 10px;height: 16px;display: block;
	 /*margin-left: -50px;*/
	 /*margin-top: 14px;*/
}

.first_Bar .header-title,.nav_bar .header-title{
	position: absolute;
	right: 40px;
    left: 40px;
    
    display: inline-block;
    overflow: hidden;

    width: auto;
    margin: 0;

    text-overflow: ellipsis;
    text-align: center;
    line-height: 44px;
    
    font-family: "arial black";
    font-size: 18px;
    color: white;
}
.nav_bar3 .header-title{
	position: absolute;
	right: 40px;
    left: 40px;
    
    display: inline-block;
    overflow: hidden;

    width: auto;
    margin: 0;

    text-overflow: ellipsis;
    text-align: center;
    line-height: 44px;
    
    font-family: "arial black";
    font-size: 18px;
    color: white;
}

.first_Bar .rightItem,.nav_bar .rightItem{
	position: absolute;
	right: 15px;
	height: 44px;line-height: 44px;
	
	border: none;
	background-color: rgba(255,255,255,0);
	font-size: 15px;
	color: white;
}
