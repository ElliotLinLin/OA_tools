
.title2 {
    margin: 2px;
    height: 45px;

    background: url(../images/version.png) no-repeat;
}

.title2 p, .title2 select {
    display: inline-block;
}

.title2 p {
    padding-bottom: 15px;
    padding-top: 15px;
    margin-left: 50px;
    color: white;
}

.title2 select {
    width: auto;
    height: 30px;
    color: white;

    border-radius: 0;
    padding: 5px;
    font-size: 15px;
    border: 1px solid #dfe1f5;
    color: white;
    background: transparent;
    margin-bottom: 0;
    vertical-align: middle;
    -webkit-appearance: none;
    background: url(../images/version_last.png) no-repeat;
    background-position: 100% center;
    background-size: 15px 15px;
    padding-right: 15px;
}

.title2 select option {
    background-color: rgb(200, 200, 200);
}

